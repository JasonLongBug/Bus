<view><view class="home-head"><view class="search"><icon type="search" size="16"></icon><input placeholder="搜索公交线路" type="number" confirm-type="search" focus="true" confirm="btnQuery_Click" data-event-opts="{{[['input',[['__set_model',['','keyword','$event',[]]],['btnQuery_Click',['$event']]]]]}}" value="{{keyword}}" bindinput="__e"/></view><view data-event-opts="{{[['tap',[['btnQuery_Click',['$event']]]]]}}" class="btnQuery" bindtap="__e">查询</view></view><block wx:if="{{busList.length>0}}"><view class="list-block"><view class="list-title">线路</view><view class="list"><block wx:for="{{busList}}" wx:for-item="item" wx:for-index="__i0__" wx:key="lineId"><view class="bus" data-lineId="{{item.lineId}}" data-event-opts="{{[['tap',[['bus_Click',['$event']]]]]}}" bindtap="__e"><image class="icon-bus" src="../../static/image/icon-bus.png"></image><view class="bus-number">{{item.lineNo+"路"}}</view><view class="bus-route">{{item.startSn+"-"+item.endSn}}</view></view></block></view></view></block></view>